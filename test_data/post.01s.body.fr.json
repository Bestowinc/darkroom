{
  "cut": {
    "from": [
      "ADDRESS",
      "BODY",
      "URI_METHOD"
    ],
    "to": {
      "trace": "'response'.'body'.'headers'.'X-Amzn-Trace-Id'",
      "IP": "'response'.'body'.'origin'"
    }
  },
  "protocol": "HTTP",
  "request": {
    "body": "the exact body that is sent",
    "uri": "${URI_METHOD} post",
    "entrypoint": "${ADDRESS}"
  },
  "response": {
    "body": {
      "args": {},
      "data": "\"${BODY}\"",
      "files": {},
      "form": {},
      "headers": {
        "Accept": "*/*",
        "Content-Length": "29",
        "Host": "httpbin.org",
        "X-Amzn-Trace-Id": "${trace}"
      },
      "json": "${BODY}",
      "origin": "${IP}",
      "url": "https://httpbin.org/post"
    },
    "status": 200
  }
}
